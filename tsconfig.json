{
	"compilerOptions": {
		"noEmit": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["ESNext", "DOM"],
		"types": ["bun"],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"outDir": "dist/",
		"resolveJsonModule": true,
		"rootDir": "lib/",
		"target": "ESNext",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": ["tsdown.config.ts", "dist/", "node_modules"]
}
